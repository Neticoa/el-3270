{
  "name": "el3270",
  "main": "dist/main.js",
  "description": "Electron-based IBM 3270 Emulator",
  "author": "Mark Florence <mflo999@gmail.com>",
  "version": "1.0.0",
  "license": "MIT",
  "build": {
    "appId": "el3270"
  },
  "scripts": {
      "build": "rimraf dist && ng build --prod --aot",
      "dist": "electron-builder --dir",
      "ng": "ng",
      "live": "rimraf dist && ng build --prod --aot && tsc -p tsconfig.main.json && electron . --ignore-gpu-blacklist",
      "markdown-toc": "markdown-toc",
      "pack": "rimraf dist && ng build --prod --aot && tsc -p tsconfig.main.json && electron-builder",
      "rimraf": "rimraf",
      "serve": "ng serve",
      "test": "tsc -p tsconfig.main.json && DEV_MODE=1 electron . --ignore-gpu-blacklist",
      "toc": "markdown-toc -i README.md",
      "tsc": "tsc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.4",
    "@ngxs/logger-plugin": "^2.0.0",
    "@ngxs/storage-plugin": "^2.0.0",
    "@ngxs/store": "^2.0.0",
    "core-js": "^2.4.1",
    "electron-capture": "^1.1.3",
    "ellib": "latest",
    "hammerjs": "^2.0.8",
    "marked": "^0.3.19",
    "moment": "^2.21.0",
    "ngx-electron": "^1.0.4",
    "numeral": "^2.0.6",
    "roboto-fontface": "^0.9.0",
    "rxjs": "^5.5.6",
    "tn3270": "latest",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.3",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "devtron": "^1.4.0",
    "electron": "^1.8.4",
    "electron-builder": "^20.10.0",
    "eslint": "^4.19.0",
    "markdown-toc": "^1.2.0",
    "rimraf": "^2.6.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
